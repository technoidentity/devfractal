{
  "name": "react-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run && vitest typecheck --run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rimraf .turbo node_modules dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch ./db.js"
  },
