{
  "name": "router-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --report-unused-disable-directives --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run && vitest typecheck --run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rimraf .turbo node_modules dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch ./db.js",
    "e2e": "pnpm playwright test --quiet",
    "e2e:dev": "pnpm playwright test --ui",
    "e2e:run": "cross-env CI=true playwright test"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/fake-tasks": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/router": "workspace:*",
    "@srtp/ui": "workspace:*",
    "@srtp/web": "workspace:*",
    "@tanstack/react-query": "4.35.7",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-router-dom": "6.16.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "use-immer": "0.9.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@playwright/test": "1.38.1",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.8.0",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@vitejs/plugin-react-swc": "3.4.0",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "msw": "1.3.2",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
