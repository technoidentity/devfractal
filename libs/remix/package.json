{
  "name": "@srtp/remix",
  "version": "1.0.0",
  "description": "remix convenience utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "upd": "pnpm ncu -u",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11.10.0",
    "@mantine/core": "5.10.3",
    "@mantine/dates": "5.10.3",
    "@mantine/form": "5.10.3",
    "@mantine/hooks": "5.10.3",
    "@mantine/modals": "5.10.3",
    "@mantine/notifications": "5.10.3",
    "@mantine/remix": "5.10.3",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "@srtp/core": "workspace:*",
    "@srtp/form": "workspace:*",
    "dayjs": "1.11.7",
    "immer": "9.0.19",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.8.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.11.19",
    "@types/react-dom": "18.0.10",
    "@types/react": "18.0.27",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "npm-check-updates": "16.6.5",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "tsup": " 6.5.0",
    "turbo": "1.7.3",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4"
  }
}
