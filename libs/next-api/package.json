{
  "name": "@srtp/api",
  "version": "1.0.0",
  "description": "convenient utility functions for defining next api",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "author": "Pervez Iqbal",
  "license": "ISC",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@tanstack/react-query": "4.32.0",
    "date-fns": "2.30.0",
    "http-status-codes": "2.2.0",
    "next": "13.4.12",
    "next-auth": "4.22.3",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redaxios": "0.5.1",
    "tslib": "2.6.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.0",
    "tsup": "7.1.0",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  }
}
