{
  "name": "@srtp/api",
  "version": "0.0.1",
  "description": "convenient utility functions for defining next api",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "date-fns": "2.30.0",
    "http-status-codes": "2.2.0",
    "next": "13.4.19",
    "next-auth": "4.23.1",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redaxios": "0.5.1",
    "tslib": "2.6.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.48.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.3"
  }
}
