{
  "name": "@srtp/remix-react",
  "version": "0.0.1",
  "description": "form components for remix",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@mantine/core": "6.0.19",
    "@mantine/dates": "6.0.19",
    "@mantine/form": "6.0.19",
    "@mantine/hooks": "6.0.19",
    "@remix-run/react": "1.19.3",
    "@srtp/core": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/remix-core": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/validator": "workspace:*",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "type-fest": "4.2.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "tsup": "7.2.0",
    "typescript": "5.1.6",
    "vitest": "0.34.2"
  }
}
