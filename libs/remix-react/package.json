{
  "name": "@srtp/remix-react",
  "version": "1.0.0",
  "description": "form components for remix",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "author": "Pervez Iqbal",
  "license": "ISC",
  "scripts": {
    "upd": "pnpm ncu -u",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.8",
    "@mantine/dates": "6.0.8",
    "@mantine/form": "6.0.8",
    "@mantine/hooks": "6.0.8",
    "@remix-run/react": "1.15.0",
    "@srtp/core": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/remix-core": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/validator": "workspace:*",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.3",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "type-fest": "3.8.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "2.8.8",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  }
}
