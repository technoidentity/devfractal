{
  "name": "devfractal",
  "version": "0.0.1",
  "description": "include all libraries from this project related to react",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/endpoint": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/query": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/result": "workspace:*",
    "@srtp/router": "workspace:*",

    "@srtp/ui": "workspace:*",
    "@srtp/ui-form": "workspace:*",
    "@srtp/web": "workspace:*",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "use-immer": "0.9.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "tsup": "7.2.0",
    "typescript": "5.1.6",
    "vitest": "0.34.2"
  }
}
