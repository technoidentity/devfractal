{
  "name": "@srtp/fake-tasks",
  "version": "0.0.1",
  "description": "fake tasks database models for all apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run && vitest typecheck --run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@types/node": "20.5.9",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "18.15.11",
    "eslint": "8.48.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.3"
  }
}
