{
  "name": "devfractal-demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "web:dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "server:dev": "tsx --watch src/server.ts",
    "dev": "concurrently pnpm:web:dev pnpm:db",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rimraf .turbo node_modules dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch db.json"
  },
