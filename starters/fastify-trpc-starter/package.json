{
  "name": "fastify-trpc-starter",
  "version": "1.0.0",
  "description": "trpc with fastify",
  "main": "index.js",
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "npx tsc -p .",
    "types": "npx tsc --noEmit -p .",
    "start": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/server.js\"",
    "types:dev": "tsc --noEmit -p . --watch",
    "tcd": "npx tsc --noEmit -p . --watch",
    "rs": "node -r esbuild-register",
    "ro": "node -r esbuild-register src/index.ts",
    "dev": "nodemon -x node -r esbuild-register src/server.ts"
  },
  "keywords": [],
  "author": "Pervez Iqbal",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/websocket": "7.2.0",
    "@trpc/server": "10.20.0",
    "close-with-grace": "1.2.0",
    "dotenv": "16.0.3",
    "esbuild": "0.17.17",
    "esbuild-register": "3.4.2",
    "eslint": "8.38.0",
    "fastify": "4.15.0",
    "fastify-autoload": "3.13.0",
    "fastify-cli": "5.7.1",
    "fastify-cors": "6.1.0",
    "fastify-plugin": "4.5.0",
    "fastify-sensible": "3.2.0",
    "fastify-websocket": "4.3.0",
    "immer": "10.0.1",
    "lodash": "4.17.21",
    "make-promises-safe": "5.1.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.7",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "trpc-openapi": "1.1.2",
    "trpc-playground": "1.0.4",
    "typescript": "5.0.4",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/lodash": "4.14.194",
    "@types/node": "18.15.11",
    "@types/tap": "15.0.8",
    "concurrently": "8.0.1",
    "cross-env": "7.0.3",
    "fastify-cli": "5.7.1",
    "fastify-tsconfig": "1.0.1",
    "npm-run-all": "4.1.5",
    "pino-pretty": "10.0.0",
    "tap": "16.3.4",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "wait-port": "1.0.4"
  }
}
