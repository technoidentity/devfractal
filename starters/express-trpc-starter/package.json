{
  "name": "express-trpc-starter",
  "version": "0.0.1",
  "description": "express trpc starter project",
  "main": "index.js",
  "scripts": {
    "start": "pnpm dev",
    "clean": "rimraf .turbo node_modules dist",
    "types": "npx tsc -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "tcd": "npx tsc -p . --watch",
    "build": "npx tsc -p .",
    "test": "npx tap --node-arg='--require=esbuild-register'",
    "test:dev": "pnpm test -- --watch",
    "rs": "node -r esbuild-register",
    "ro": "node -r esbuild-register src/index.ts",
    "dev": "nodemon -x node -r esbuild-register src/index.ts",
    "validate": "pnpm turbo build test"
  },
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "10.38.4",
    "chance": "1.1.11",
    "express": "4.18.2",
    "got": "13.0.0",
    "immer": "10.0.2",
    "superjson": "1.13.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@types/chance": "1.1.4",
    "@types/express": "4.17.18",
    "@types/express-serve-static-core": "4.17.37",
    "@types/node": "20.7.0",
    "@types/qs": "6.9.8",
    "@types/tap": "15.0.9",
    "esbuild": "0.19.3",
    "esbuild-register": "3.5.0",
    "eslint": "8.50.0",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "start-server-and-test": "2.0.1",
    "tap": "18.1.5",
    "trpc-openapi": "1.2.0",
    "trpc-panel": "1.3.4",
    "trpc-playground": "1.0.4",
    "tsx": "3.13.0",
    "turbo": "1.10.14",
    "typescript": "5.2.2",
    "vitest": "0.34.5",
    "wait-port": "1.1.0"
  }
}
