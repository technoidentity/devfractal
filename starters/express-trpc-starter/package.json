{
  "name": "express-trpc-starter",
  "version": "1.0.0",
  "description": "express trpc starter project",
  "main": "index.js",
  "scripts": {
    "start": "pnpm dev",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "types": "npx tsc -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "tcd": "npx tsc -p . --watch",
    "build": "npx tsc -p .",
    "test": "npx tap --node-arg='--require=esbuild-register'",
    "test:dev": "pnpm test -- --watch",
    "rs": "node -r esbuild-register",
    "ro": "node -r esbuild-register src/index.ts",
    "dev": "nodemon -x node -r esbuild-register src/index.ts",
    "cd": "pnpm turbo build test"
  },
  "author": "Pervez Iqbal",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "10.35.0",
    "chance": "1.1.11",
    "express": "4.18.2",
    "got": "13.0.0",
    "immer": "10.0.2",
    "lodash": "4.17.21",
    "superjson": "1.13.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/express": "4.17.17",
    "@types/express-serve-static-core": "4.17.35",
    "@types/lodash": "4.14.195",
    "@types/node": "20.4.2",
    "@types/qs": "6.9.7",
    "@types/tap": "15.0.8",
    "esbuild": "0.18.15",
    "esbuild-register": "3.4.2",
    "eslint": "8.45.0",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "start-server-and-test": "2.0.0",
    "tap": "16.3.7",
    "trpc-openapi": "1.2.0",
    "trpc-panel": "1.3.4",
    "trpc-playground": "1.0.4",
    "tsx": "3.12.7",
    "turbo": "1.10.9",
    "typescript": "5.1.6",
    "vitest": "0.33.0",
    "wait-port": "1.0.4"
  }
}
