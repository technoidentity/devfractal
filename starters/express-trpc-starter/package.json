{
  "name": "express-trpc-starter",
  "version": "1.0.0",
  "description": "express trpc starter project",
  "main": "index.js",
  "scripts": {
    "start": "pnpm dev",
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "types": "npx tsc -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "tcd": "npx tsc -p . --watch",
    "build": "npx tsc -p .",
    "test": "npx tap --node-arg='--require=esbuild-register'",
    "test:dev": "pnpm test -- --watch",
    "rs": "node -r esbuild-register",
    "ro": "node -r esbuild-register src/index.ts",
    "dev": "nodemon -x node -r esbuild-register src/index.ts",
    "ci": "pnpm turbo build test"
  },
  "author": "Pervez Iqbal",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "10.9.1",
    "chance": "1.1.9",
    "express": "4.18.2",
    "got": "12.5.3",
    "immer": "9.0.19",
    "lodash": "4.17.21",
    "superjson": "1.12.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/express": "4.17.16",
    "@types/express-serve-static-core": "4.17.33",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/qs": "6.9.7",
    "@types/tap": "15.0.7",
    "esbuild": "0.17.5",
    "esbuild-register": "3.4.2",
    "eslint": "8.33.0",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "start-server-and-test": "1.15.3",
    "tap": "16.3.4",
    "trpc-openapi": "1.1.2",
    "trpc-panel": "1.2.7",
    "trpc-playground": "1.0.4",
    "tsx": "3.12.2",
    "turbo": "1.7.0",
    "typescript": "4.9.5",
    "vitest": "0.28.3",
    "wait-port": "1.0.4"
  }
}
