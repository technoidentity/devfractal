{
  "name": "t3-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:types": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "pnpm exec prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm exec prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm exec prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm exec prisma studio",
    "db:repl": "dotenv -e .env.development pnpm exec prisma-repl",
    "postinstall": "pnpm db:types",
    "build": "pnpm db:types && next build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "coverage": "vitest run --coverage",
    "dev": "next dev",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "next lint",
    "start": "next start",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "upd": "pnpm ncu -u"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.5",
    "@prisma/client": "4.9.0",
    "@tanstack/react-query": "4.22.0",
    "@trpc/client": "10.9.0",
    "@trpc/next": "10.9.0",
    "@trpc/react-query": "10.9.0",
    "@trpc/server": "10.9.0",
    "next": "13.1.2",
    "next-auth": "4.18.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "4.2.1",
    "prisma": "4.9.0",
    "typescript": "4.9.4",
    "vitest": "^0.27.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.3.1"
  }
}
