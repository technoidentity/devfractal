{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"./app/**/*.{js,ts,tsx}\"",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "ci": "pnpm turbo lint types"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11",
    "@mantine/core": "5.10.3",
    "@mantine/dates": "5.10.3",
    "@mantine/form": "5.10.3",
    "@mantine/hooks": "5.10.3",
    "@mantine/modals": "5.10.3",
    "@mantine/notifications": "5.10.3",
    "@mantine/remix": "5.10.3",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "dayjs": "1.11.7",
    "isbot": "3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "13.4.0",
    "@remix-run/dev": "1.12.0",
