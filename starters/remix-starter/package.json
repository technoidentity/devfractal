    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/node": "18.11.19",
    "eslint": "8.33.0",
    "cross-env": "7.0.3",
    "npm-check-updates": "16.6.5",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "turbo": "1.7.3",
    "vite": "4.1.1",
    "vitest": "0.28.4",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
