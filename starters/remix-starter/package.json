{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint --max-warnings=0 --cache app --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "cd": "pnpm turbo lint types"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.13",
    "@mantine/dates": "6.0.13",
    "@mantine/form": "6.0.13",
    "@mantine/hooks": "6.0.13",
    "@mantine/modals": "6.0.13",
    "@mantine/notifications": "6.0.13",
    "@mantine/remix": "6.0.13",
    "@remix-run/node": "1.16.1",
    "@remix-run/react": "1.16.1",
    "@remix-run/serve": "1.16.1",
    "dayjs": "1.11.7",
    "isbot": "3.6.10",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.16.1",
    "@remix-run/eslint-config": "1.16.1",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "turbo": "1.10.1",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.31.4"
  },
  "engines": {
    "node": ">=14"
  }
