{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint --max-warnings=0 --cache .",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "cd": "pnpm turbo lint types"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/server": "11",
    "@mantine/core": "6.0.2",
    "@mantine/dates": "6.0.2",
    "@mantine/form": "6.0.2",
    "@mantine/hooks": "6.0.2",
    "@mantine/modals": "6.0.2",
    "@mantine/notifications": "6.0.2",
    "@mantine/remix": "6.0.2",
    "@remix-run/node": "1.14.1",
    "@remix-run/react": "1.14.1",
    "@remix-run/serve": "1.14.1",
    "dayjs": "1.11.7",
    "isbot": "3.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@remix-run/dev": "1.14.1",
    "@remix-run/eslint-config": "1.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/node": "18.15.3",
    "eslint": "8.36.0",
    "cross-env": "7.0.3",
    "npm-check-updates": "16.7.12",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "turbo": "1.8.3",
    "vite": "4.1.4",
    "vitest": "0.29.2",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
