{
  "name": "@ti/vanilla-fastify",
  "private": true,
  "description": "basic fastify server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "dev": "tsx --watch src/index.ts",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "3.3.0",
    "@sinclair/typebox": "0.29.6",
    "fastify": "4.20.0"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/node-lts": "18.12.3",
    "@tsconfig/node18": "18.2.0",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.4.4",
    "eslint": "8.45.0",
    "eslint-config-srtp": "workspace:*",
    "pino-pretty": "10.1.0",
    "prettier": "3.0.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
