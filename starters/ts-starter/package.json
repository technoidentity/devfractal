{
  "name": "ts-starter",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "cd": "pnpm turbo lint types test"
  },
  "dependencies": {
    "immer": "10.0.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-turbo": "1.10.7",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "turbo": "1.10.7",
    "typescript": "5.1.6",
    "vite": "4.4.3",
    "vitest": "0.33.0"
  }
}
