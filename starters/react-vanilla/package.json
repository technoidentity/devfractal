    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "react-hot-toast": "2.4.1",
    "swr": "2.1.5",
    "tiny-invariant": "1.3.1",
    "tslib": "2.5.2",
    "use-immer": "0.9.0",
    "wretch": "2.5.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.1",
    "@tsconfig/vite-react": "2.0.0",
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vitejs/plugin-react-swc": "3.3.1",
    "eslint": "8.41.0",
    "eslint-config-srtp": "workspace:*",
