{
  "name": "@ti/ts",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "cd": "turbo types lint test",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "npx parcel --lazy 'pages/*.html'",
    "lint": "eslint --fix --max-warnings=0 --cache src --ext .ts",
    "test:dev": "vitest",
    "test": "vitest run --passWithNoTests",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p ."
  },
  "dependencies": {
    "immer": "10.0.2",
    "remeda": "1.17.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/strictest": "2.0.1",
    "@types/node": "20.2.3",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "eslint": "8.41.0",
    "parcel": "2.8.3",
    "prettier": "2.8.8",
    "turbo": "1.9.8",
    "typescript": "5.0.4"
  }
}
