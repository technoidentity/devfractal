{
  "name": "next-trpc-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm db:types && next build",
    "dev": "next dev",
    "postinstall": "pnpm db:types",
    "lint": "next lint",
    "start": "next start",
    "clean": "rimraf .turbo node_modules dist",
    "dev!": "pnpm db! && pnpm dev",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "db:types": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:seed": "pnpm prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "pnpm prisma migrate dev --create-only",
    "db:studio": "pnpm prisma studio",
    "db:repl": "pnpm prisma-repl",
    "cd": "pnpm turbo types lint"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.19",
    "@mantine/dates": "6.0.19",
    "@mantine/form": "6.0.19",
    "@mantine/hooks": "6.0.19",
    "@mantine/modals": "6.0.19",
    "@mantine/next": "6.0.19",
    "@mantine/notifications": "6.0.19",
    "@mantine/nprogress": "6.0.19",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/font": "13.4.13",
    "@prisma/client": "5.1.1",
    "@tanstack/react-query": "4.32.6",
    "@trpc/client": "10.37.1",
    "@trpc/next": "10.37.1",
    "@trpc/react-query": "10.37.1",
    "@trpc/server": "10.37.1",
    "dayjs": "1.11.9",
    "immer": "10.0.2",
    "jotai": "2.3.1",
    "jotai-tanstack-query": "0.7.1",
    "next": "13.4.13",
    "next-auth": "4.23.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "superjson": "1.13.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.1",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.13",
    "prettier": "3.0.1",
    "prisma": "5.1.1",
    "rimraf": "5.0.1",
    "turbo": "1.10.12",
    "typescript": "5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.3.2"
  }
}
