{
  "name": "next-trpc-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm db:types && next build",
    "dev": "next dev",
    "postinstall": "pnpm db:types",
    "lint": "next lint",
    "start": "next start",
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev!": "pnpm db! && pnpm dev",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "db:types": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "pnpm exec prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm exec prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "pnpm exec prisma migrate dev --create-only",
    "db:studio": "pnpm exec prisma studio",
    "db:repl": "pnpm exec prisma-repl",
    "ci": "pnpm turbo types lint"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11",
    "@mantine/core": "5.10.3",
    "@mantine/dates": "5.10.3",
    "@mantine/form": "5.10.3",
    "@mantine/hooks": "5.10.3",
    "@mantine/modals": "5.10.3",
    "@mantine/next": "5.10.3",
    "@mantine/notifications": "5.10.3",
    "@mantine/nprogress": "5.10.3",
    "@next-auth/prisma-adapter": "1.0.5",
    "@next/font": "13.1.6",
    "@prisma/client": "4.10.1",
    "@tanstack/react-query": "4.24.4",
    "@trpc/client": "10.11.0",
    "@trpc/next": "10.11.0",
    "@trpc/react-query": "10.11.0",
    "@trpc/server": "10.11.0",
    "dayjs": "1.11.7",
    "immer": "9.0.19",
    "jotai": "2.0.1",
    "jotai-tanstack-query": "0.5.0",
    "next": "13.1.6",
    "next-auth": "4.19.2",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "superjson": "1.12.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.8.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "npm-check-updates": "16.7.4",
    "prettier": "2.8.4",
    "prisma": "4.10.1",
    "rimraf": "4.1.2",
    "turbo": "1.7.4",
    "typescript": "4.9.5"
  },
  "ct3aMetadata": {
    "initVersion": "7.3.2"
  }
}
