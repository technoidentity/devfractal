{
  "name": "@srtp/vite-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0 --cache --cache-location ./node_modules/.cache/eslint src tests --ext ts,tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "ci": "pnpm turbo lint types test"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "5.10.5",
    "@mantine/dates": "5.10.5",
    "@mantine/form": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@tanstack/react-query": "4.24.10",
    "dayjs": "1.11.7",
    "immer": "9.0.19",
    "jotai": "2.0.2",
    "jotai-tanstack-query": "0.5.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.8.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.5.0",
    "use-immer": "0.8.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.2",
    "@types/react-dom": "18.0.11",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-react-swc": "3.2.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint": "8.35.0",
    "npm-check-updates": "16.7.10",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "turbo": "1.8.3",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  }
}
