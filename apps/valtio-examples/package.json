{
  "name": "valtio-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "upd": "pnpm ncu -u",
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "coverage": "vitest run --coverage",
    "db": "json-server ./db.js",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "serve": "vite preview",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false"
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "@chakra-ui/styled-system": "2.8.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@srtp/core": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/todo": "workspace:*",
    "framer-motion": "10.12.2",
    "immer": "9.0.21",
    "jotai": "2.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.9.0",
    "valtio": "1.10.4",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.10.8",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.30.1"
  }
}
