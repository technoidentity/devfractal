{
  "name": "beginner-programs",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "lint:fix": "pnpm lint --quiet --fix",
    "types:ci": "tsc --noEmit -p .",
    "types": "tsc --noEmit -p . --watch",
    "ci": "pnpm types:ci && pnpm lint:fix && vitest run",
    "test": "vitest dev",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "immer": "9.0.16",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.4.1",
    "zod": "3.19.1"
  },
  "exclude": [
    "**/tests/*.test.ts"
  ],
  "types": [
    "@types/jest"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "44.0.2",
    "eslint-plugin-vitest": "^0.0.11",
    "jest": "^29.2.2",
    "npm-check-updates": "16.3.16",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vitest": "0.24.4"
  }
}
