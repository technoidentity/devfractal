{
  "name": "strp-demo",
  "version": "0.0.1",
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "cd": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.7",
    "@mantine/dates": "6.0.7",
    "@mantine/form": "6.0.7",
    "@mantine/hooks": "6.0.7",
    "@mantine/notifications": "6.0.7",
    "@tanstack/react-query": "4.29.1",
    "dayjs": "1.11.7",
    "immer": "9.0.21",
    "jotai": "2.0.4",
    "jotai-tanstack-query": "0.7.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "react-router-dom": "6.10.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.10.8",
    "prettier": "2.8.7",
    "rimraf": "5.0.0",
    "turbo": "1.9.1",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.30.1"
  }
}
