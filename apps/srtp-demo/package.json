{
  "name": "strp-demo",
  "version": "0.0.1",
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "ci": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@mantine/core": "5.10.3",
    "@mantine/dates": "5.10.3",
    "@mantine/form": "5.10.3",
    "@mantine/hooks": "5.10.3",
    "@mantine/notifications": "5.10.3",
    "@tanstack/react-query": "4.24.4",
    "dayjs": "1.11.7",
    "immer": "9.0.19",
    "jotai": "2.0.0",
    "jotai-tanstack-query": "0.5.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.8.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.5.0",
    "use-immer": "0.8.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "13.4.0",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vitejs/plugin-react-swc": "3.1.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.6.5",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "turbo": "1.7.3",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4"
  }
}
