{
  "name": "strp-demo",
  "version": "0.0.1",
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "cd": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.2",
    "@mantine/dates": "6.0.2",
    "@mantine/form": "6.0.2",
    "@mantine/hooks": "6.0.2",
    "@mantine/notifications": "6.0.2",
    "@tanstack/react-query": "4.26.1",
    "dayjs": "1.11.7",
    "immer": "9.0.19",
    "jotai": "2.0.3",
    "jotai-tanstack-query": "0.6.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.9.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.8.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "@vitejs/plugin-react-swc": "3.2.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.7.12",
    "prettier": "2.8.4",
    "rimraf": "4.4.0",
    "turbo": "1.8.3",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  }
}
