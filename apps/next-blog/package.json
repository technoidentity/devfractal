{
  "name": "next-blog",
  "version": "0.0.1",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "coverage": "vitest run --coverage",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache pages components src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "glob": "10.3.3",
    "gray-matter": "4.0.3",
    "next": "13.4.9",
    "next-mdx-remote": "4.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "rehype": "12.0.1",
    "rehype-autolink-headings": "6.1.1",
    "rehype-highlight": "6.0.0",
    "rehype-slug": "5.1.0",
    "tiny-invariant": "1.3.1",
    "tslib": "2.6.0"
  },
  "devDependencies": {
    "@previewjs/config-helper-nextjs": "1.0.20",
    "@srtp/tsconfig": "workspace:*",
    "@types/glob": "8.1.0",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.44.0",
    "eslint-config-srtp": "workspace:*",
    "typescript": "5.1.6"
  }
}
