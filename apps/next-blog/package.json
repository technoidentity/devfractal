{
  "name": "next-blog",
  "version": "0.0.1",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "upd": "pnpm ncu -u",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings=0 --cache pages components src --ext ts,tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "glob": "9.0.1",
    "gray-matter": "4.0.3",
    "next": "latest",
    "next-mdx-remote": "4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "rehype": "12.0.1",
    "rehype-autolink-headings": "6.1.1",
    "rehype-highlight": "6.0.0",
    "rehype-slug": "5.1.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@previewjs/config-helper-nextjs": "1.0.6",
    "@srtp/tsconfig": "workspace:*",
    "@types/glob": "8.1.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.7.10",
    "typescript": "4.9.5"
  }
}
