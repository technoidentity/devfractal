{
  "name": "course-website",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "db:types": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "pnpm exec prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm exec prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm exec prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm exec prisma studio",
    "db:repl": "dotenv -e .env.development pnpm exec prisma-repl",
    "studio": "pnpm db:studio",
    "postinstall": "pnpm db:types",
    "build": "remix build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "pnpm db:types && remix dev",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "eslint \"app/**/*.ts*\"",
    "rs": "node -r esbuild-register",
    "start": "remix-serve build",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "upd": "pnpm ncu -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "2.4.9",
    "@emotion/cache": "11.10.5",
    "@emotion/react": "11",
    "@emotion/server": "11",
    "@emotion/styled": "11",
    "@prisma/client": "4.9.0",
    "@remix-run/node": "1.11.1",
    "@remix-run/react": "1.11.1",
    "@remix-run/serve": "1.11.1",
    "bcryptjs": "2.4.3",
    "chakra-ui-markdown-renderer": "4.1.0",
    "faker": "6.6.6",
    "framer-motion": "8.5.2",
    "isbot": "3.6.5",
    "mdx-bundler": "9.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-icons": "4.7.1",
    "rehype-highlight": "6.0.0",
    "remark-frontmatter": "4.0.1",
    "remix-auth": "3.4.0",
    "remix-auth-form": "1.3.0",
    "tiny-invariant": "1.3.1",
    "uuid": "9.0.0"
  },
  "prisma": {
    "seed": "node -r esbuild-register ./prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@remix-run/dev": "1.11.1",
    "@remix-run/eslint-config": "1.11.1",
    "@tailwindcss/typography": "0.5.9",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "9.0.0",
    "Faker": "0.7.2",
    "cross-env": "7.0.3",
    "esbuild": "0.17.4",
    "esbuild-register": "3.4.2",
    "eslint": "8.32.0",
    "lodash": "4.17.21",
    "prettier": "2.8.3",
    "prisma": "4.9.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
