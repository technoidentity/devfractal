{
  "name": "course-website",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "db:types": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:seed": "pnpm prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm prisma studio",
    "db:repl": "dotenv -e .env.development pnpm prisma-repl",
    "studio": "pnpm db:studio",
    "postinstall": "pnpm db:types",
    "build": "remix build",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "pnpm db:types && remix dev",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix app --ext .ts,.tsx",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache app --ext .ts,.tsx",
    "rs": "node -r esbuild-register",
    "start": "remix-serve build",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "upd": "pnpm ncu -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/react": "2.8.0",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@prisma/client": "5.1.1",
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "bcryptjs": "2.4.3",
    "chakra-ui-markdown-renderer": "4.1.0",
    "framer-motion": "10.15.2",
    "isbot": "3.6.13",
    "mdx-bundler": "9.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-icons": "4.10.1",
    "rehype-highlight": "6.0.0",
    "remark-frontmatter": "4.0.1",
    "remix-auth": "3.5.0",
    "remix-auth-form": "1.3.0",
    "tailwindcss": "3.3.3",
    "tiny-invariant": "1.3.1",
    "uuid": "9.0.0"
  },
  "prisma": {
    "seed": "node -r esbuild-register ./prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@remix-run/dev": "1.19.3",
    "@remix-run/eslint-config": "1.19.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "9.0.2",
    "cross-env": "7.0.3",
    "esbuild": "0.19.2",
    "esbuild-register": "3.4.2",
    "eslint": "8.47.0",
    "lodash": "4.17.21",
    "prettier": "3.0.1",
    "prisma": "5.1.1",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14"
  }
}
