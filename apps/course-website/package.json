{
  "name": "course-website",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "db:types": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:seed": "pnpm prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm prisma studio",
    "db:repl": "dotenv -e .env.development pnpm prisma-repl",
    "studio": "pnpm db:studio",
    "postinstall": "pnpm db:types",
    "build": "remix build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "pnpm db:types && remix dev",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix app --ext .ts,.tsx",
    "lint": "eslint --max-warnings=0 --cache app --ext .ts,.tsx",
    "rs": "node -r esbuild-register",
    "start": "remix-serve build",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "upd": "pnpm ncu -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.18",
    "@chakra-ui/react": "2.5.5",
    "@emotion/cache": "11.10.7",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@prisma/client": "4.13.0",
    "@remix-run/node": "1.15.0",
    "@remix-run/react": "1.15.0",
    "@remix-run/serve": "1.15.0",
    "bcryptjs": "2.4.3",
    "chakra-ui-markdown-renderer": "4.1.0",
    "framer-motion": "10.12.4",
    "isbot": "3.6.8",
    "mdx-bundler": "9.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "react-icons": "4.8.0",
    "rehype-highlight": "6.0.0",
    "remark-frontmatter": "4.0.1",
    "remix-auth": "3.4.0",
    "remix-auth-form": "1.3.0",
    "tailwindcss": "3.3.1",
    "tiny-invariant": "1.3.1",
    "uuid": "9.0.0"
  },
  "prisma": {
    "seed": "node -r esbuild-register ./prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@remix-run/dev": "1.15.0",
    "@remix-run/eslint-config": "1.15.0",
    "@tailwindcss/typography": "0.5.9",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "9.0.1",
    "cross-env": "7.0.3",
    "esbuild": "0.17.17",
    "esbuild-register": "3.4.2",
    "eslint": "8.38.0",
    "lodash": "4.17.21",
    "prettier": "2.8.7",
    "prisma": "4.13.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
