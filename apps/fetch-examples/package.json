{
  "name": "fetch-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "clean": "rimraf .turbo node_modules dist",
    "coverage": "vitest run --coverage",
    "db": "json-server --port 8080 --watch ./db.js",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "dev": "concurrently pnpm:web:dev pnpm:db",
    "dev:mock": "cross-env VITE_MOCK=1 pnpm web:dev",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "serve": "vite preview",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "web:dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/react": "2.8.0",
    "@chakra-ui/styled-system": "2.9.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@react-spring/web": "9.7.3",
    "@srtp/core": "workspace:*",
    "@srtp/endpoint": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/global-state": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/query": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/todo": "workspace:*",
    "@srtp/validator": "workspace:*",
    "@srtp/web": "workspace:*",
    "@tanstack/react-query": "4.32.0",
    "framer-motion": "10.15.0",
    "immer": "10.0.2",
    "jotai": "2.2.3",
    "jotai-tanstack-query": "0.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-virtualized-auto-sizer": "1.0.20",
    "react-window": "1.8.9",
    "react-window-infinite-loader": "1.0.9",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.1",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-beautiful-dnd": "13.1.4",
    "@types/react-dom": "18.2.7",
    "@types/react-virtualized-auto-sizer": "1.0.1",
    "@types/react-window": "1.8.5",
    "@types/react-window-infinite-loader": "1.0.6",
    "@vitejs/plugin-react-swc": "3.3.2",
    "concurrently": "8.2.0",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-srtp": "workspace:*",
    "json-server": "0.17.3",
    "msw": "1.2.3",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
