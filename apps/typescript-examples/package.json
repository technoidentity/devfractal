{
  "name": "typescript-examples",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "upd": "pnpm ncu -u",
    "build": "vite build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "coverage": "vitest run --coverage",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "eslint --max-warnings=0 --cache src --ext .ts,.tsx",
    "serve": "vite preview",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "dev": "vite --clearScreen false",
    "cd": "pnpm turbo run --parallel lint types test"
  },
  "dependencies": {
    "@preact/signals-core": "1.3.0",
    "@srtp/fn": "workspace:*",
    "@srtp/spec": "workspace:*",
    "immer": "10.0.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "18.16.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "@vitest/coverage-c8": "0.30.1",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vite-plugin-checker": "0.5.6",
    "vitest": "0.30.1"
  }
}
