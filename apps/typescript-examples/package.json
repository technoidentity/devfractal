{
  "name": "typescript-examples",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf .turbo node_modules dist",
    "coverage": "vitest run --coverage",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src --ext .ts,.tsx",
    "serve": "vite preview",
    "test:dev": "vitest dev",
    "test": "vitest run && vitest typecheck --run",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "dev": "vite --clearScreen false",
    "validate": "pnpm turbo run --parallel lint types test"
  },
  "dependencies": {
    "@preact/signals-core": "1.4.0",
    "@srtp/fn": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/web": "workspace:*",
    "immer": "10.0.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@srtp/tsconfig": "workspace:*",
    "@types/node": "20.5.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-v8": "0.34.2",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2",
    "vitest": "0.34.2"
  }
}
