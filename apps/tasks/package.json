{
  "name": "tasks",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "lint": "eslint --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "start": "cross-env NODE_ENV=production node ./server.js"
  },
  "dependencies": {
    "@remix-run/express": "1.15.0",
    "@remix-run/node": "1.15.0",
    "@remix-run/react": "1.15.0",
    "better-sqlite3": "^8.3.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "drizzle-orm": "^0.24.4",
    "express": "4.18.2",
    "immer": "10.0.1",
    "isbot": "3.6.10",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "type-fest": "3.8.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.15.0",
    "@remix-run/eslint-config": "1.15.0",
    "@types/better-sqlite3": "^7.6.4",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "dotenv": "16.0.3",
    "drizzle-kit": "^0.17.5",
    "eslint": "8.39.0",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
