{
  "name": "tasks",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "lint": "eslint --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "start": "cross-env NODE_ENV=production node ./server.js"
  },
  "dependencies": {
    "@remix-run/express": "1.16.1",
    "@remix-run/node": "1.16.1",
    "@remix-run/react": "1.16.1",
    "better-sqlite3": "8.4.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "drizzle-orm": "0.26.3",
    "express": "4.18.2",
    "immer": "10.0.2",
    "isbot": "3.6.10",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.2",
    "type-fest": "3.11.1",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.16.1",
    "@remix-run/eslint-config": "1.16.1",
    "@types/better-sqlite3": "7.6.4",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.17",
    "@types/morgan": "1.9.4",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "dotenv": "16.1.3",
    "drizzle-kit": "0.18.1",
    "eslint": "8.41.0",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=14"
  }
}
