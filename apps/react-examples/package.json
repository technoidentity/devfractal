{
  "name": "react-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "upd": "pnpm ncu -u",
    "build": "vite build",
    "web:dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false",
    "dev": "concurrently pnpm:web:dev pnpm:db",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server ./db.js"
  },
  "dependencies": {
    "@chakra-ui/react": "2.5.5",
    "@chakra-ui/styled-system": "2.8.0",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/spec": "workspace:*",
    "framer-motion": "10.12.4",
    "immer": "10.0.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "redaxios": "0.5.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "type-fest": "3.8.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.3.0",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vitest": "0.30.1"
  }
}
