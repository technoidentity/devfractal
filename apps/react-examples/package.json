{
  "name": "react-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rimraf .turbo node_modules dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch ./db.js"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.0",
    "@chakra-ui/styled-system": "2.9.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@formkit/auto-animate": "0.7.0",
    "@mantine/form": "6.0.17",
    "@mantine/hooks": "6.0.17",
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/global-state": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/query": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/web": "workspace:*",
    "@tanstack/react-query": "4.32.0",
    "chance": "1.1.11",
    "exhaustive": "1.1.1",
    "framer-motion": "10.13.1",
    "immer": "10.0.2",
    "jotai": "2.2.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.1",
    "type-fest": "4.0.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/chance": "1.1.3",
    "@types/node": "20.4.4",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "json-server": "0.17.3",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  }
}
