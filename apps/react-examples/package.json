{
  "name": "react-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm"
  },
  "dependencies": {
    "@chakra-ui/react": "2.7.0",
    "@chakra-ui/styled-system": "2.9.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mantine/form": "6.0.13",
    "@mantine/hooks": "6.0.13",
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/global-state": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@tanstack/react-query": "4.29.12",
    "framer-motion": "10.12.16",
    "immer": "10.0.2",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "redaxios": "0.5.1",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.3",
    "type-fest": "3.11.1",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.32.0"
  }
}
