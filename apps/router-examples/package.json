{
  "name": "react-router-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "web:dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "dev": "concurrently pnpm:web:dev pnpm:db",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch db.json"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.0",
    "@chakra-ui/styled-system": "2.9.1",
    "@conform-to/react": "0.7.4",
    "@conform-to/zod": "0.7.4",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@srtp/conform": "workspace:*",
    "@srtp/core": "workspace:*",
    "@srtp/endpoint": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/global-state": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/query": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/router": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/validator": "workspace:*",
    "@srtp/web": "workspace:*",
    "@tanstack/react-query": "4.30.0",
    "framer-motion": "10.13.0",
    "immer": "10.0.2",
    "jotai": "2.2.2",
    "localforage": "1.10.0",
    "match-sorter": "6.3.1",
    "prop-types": "15.8.1",
    "query-string": "8.1.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-router-dom": "6.14.2",
    "react": "18.2.0",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.4.2",
    "@types/react-dom": "18.2.7",
    "@types/react": "18.2.15",
    "@vitejs/plugin-react": "4.0.3",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "eslint": "8.45.0",
    "json-server": "0.17.3",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.6",
    "vitest": "0.33.0"
  }
}
