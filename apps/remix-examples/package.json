{
  "name": "remix-examples",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "fly deploy --remote-only",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "clean": "rimraf .turbo node_modules dist",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache app --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run --passWithNoTests",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "validate": "pnpm turbo lint types"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.19",
    "@mantine/dates": "6.0.19",
    "@mantine/form": "6.0.19",
    "@mantine/hooks": "6.0.19",
    "@mantine/modals": "6.0.19",
    "@mantine/notifications": "6.0.19",
    "@mantine/remix": "6.0.19",
    "@remix-run/node": "1.19.3",
    "@remix-run/react": "1.19.3",
    "@remix-run/serve": "1.19.3",
    "dayjs": "1.11.9",
    "isbot": "3.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.3",
    "@remix-run/eslint-config": "1.19.3",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "turbo": "1.10.12",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitest": "0.34.2"
  },
  "engines": {
    "node": ">=14"
  }
}
