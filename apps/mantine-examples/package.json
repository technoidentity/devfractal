{
  "name": "mantine-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "cd": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@mantine/core": "6.0.19",
    "@mantine/dates": "6.0.19",
    "@mantine/form": "6.0.19",
    "@mantine/hooks": "6.0.19",
    "@mantine/notifications": "6.0.19",
    "@srtp/core": "workspace:*",
    "@srtp/table": "workspace:*",
    "@tanstack/query-core": "4.32.6",
    "@tanstack/react-query": "4.32.6",
    "dayjs": "1.11.9",
    "immer": "10.0.2",
    "jotai": "2.3.1",
    "jotai-tanstack-query": "0.7.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-router-dom": "6.15.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.1",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/lodash": "4.14.197",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint": "8.47.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "3.0.1",
    "rimraf": "5.0.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitest": "0.34.1"
  }
}
