{
  "name": "mantine-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "cd": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.8",
    "@mantine/dates": "6.0.8",
    "@mantine/form": "6.0.8",
    "@mantine/hooks": "6.0.8",
    "@mantine/notifications": "6.0.8",
    "@srtp/core": "workspace:*",
    "@srtp/table": "workspace:*",
    "@tanstack/query-core": "4.29.1",
    "@tanstack/react-query": "4.29.3",
    "dayjs": "1.11.7",
    "immer": "10.0.1",
    "jotai": "2.0.4",
    "jotai-tanstack-query": "0.7.0",
    "lodash": "4.17.21",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.4",
    "react-router-dom": "6.10.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.5.0",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/lodash": "4.14.194",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-srtp": "workspace:*",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vitest": "0.30.1"
  }
}
