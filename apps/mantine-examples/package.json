{
  "name": "mantine-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "upd": "pnpm ncu -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint \"./src/**/*.{js,ts,tsx}\"",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "ci": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@mantine/core": "5.10.1",
    "@mantine/dates": "5.10.1",
    "@mantine/form": "5.10.1",
    "@mantine/hooks": "5.10.1",
    "@mantine/notifications": "5.10.1",
    "@srtp/core": "workspace:*",
    "@tanstack/query-core": "4.22.4",
    "@tanstack/react-query": "4.22.4",
    "dayjs": "1.11.7",
    "immer": "9.0.18",
    "jotai": "1.13.1",
    "jotai-tanstack-query": "0.4.0",
    "lodash": "4.17.21",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.7.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-essentials": "9.3.0",
    "tslib": "2.4.1",
    "use-immer": "0.8.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "13.4.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react-swc": "3.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.32.0",
    "eslint-config-srtp": "workspace:*",
    "npm-check-updates": "16.6.2",
    "prettier": "2.8.3",
    "rimraf": "4.1.1",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.27.3"
  }
}
