{
  "name": "ti-finance-dd14",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "boot": "pnpm docker && pnpm db! && pnpm dev",
    "build": "run-s build:*",
    "clean": "pnpm rimraf .turbo node_modules dist public/build",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "pnpm build && run-p dev:*",
    "start:dev": "pnpm docker && pnpm db! && pnpm dev",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache app --ext .ts,.tsx",
    "setup": "pnpm db:types && pnpm prisma migrate deploy && pnpm prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "db:types": "pnpm prisma generate",
    "db:push": "pnpm prisma db push",
    "db:seed": "pnpm prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm prisma studio",
    "db:repl": "dotenv -e .env.development pnpm prisma-repl",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@mantine/core": "6.0.20",
    "@mantine/dates": "6.0.20",
    "@mantine/form": "6.0.20",
    "@mantine/hooks": "6.0.20",
    "@mantine/modals": "7.1.0",
    "@mantine/notifications": "6.0.20",
    "@mantine/remix": "6.0.21",
    "@prisma/client": "5.3.1",
    "@remix-run/express": "2.0.1",
    "@remix-run/node": "2.0.1",
    "@remix-run/react": "2.0.1",
    "@remix-run/server-runtime": "2.0.1",
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/mantine": "workspace:*",
    "@srtp/query": "workspace:*",
    "@srtp/react": "workspace:*",
    "@srtp/remix-core": "workspace:*",
    "@srtp/remix-node": "workspace:*",
    "@srtp/remix-react": "workspace:*",
    "@tabler/icons-react": "2.38.0",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dayjs": "1.11.10",
    "devalue": "4.3.2",
    "express": "4.18.2",
    "express-prometheus-middleware": "1.2.0",
    "http-status-codes": "2.3.0",
    "isbot": "3.7.0",
    "modern-errors": "6.0.0",
    "morgan": "1.10.0",
    "prom-client": "14.2.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.3",
    "tiny-invariant": "1.3.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@remix-run/dev": "2.0.1",
    "@remix-run/eslint-config": "2.0.1",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/bcryptjs": "2.4.4",
    "@types/compression": "1.7.3",
    "@types/eslint": "8.44.3",
    "@types/express": "4.17.18",
    "@types/express-prometheus-middleware": "1.2.1",
    "@types/morgan": "1.9.6",
    "@types/node": "20.8.0",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-v8": "0.34.6",
    "autoprefixer": "10.4.16",
    "c8": "8.0.1",
    "cookie": "0.5.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.4",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "happy-dom": "12.5.0",
    "msw": "1.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "prisma": "5.3.1",
    "rimraf": "5.0.5",
    "start-server-and-test": "2.0.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
