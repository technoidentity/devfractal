{
  "name": "ti-finance-dd14",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "boot": "pnpm docker && pnpm db! && pnpm dev",
    "upd": "pnpm ncu -u",
    "build": "run-s build:*",
    "clean": "pnpm rimraf .turbo node_modules dist public/build",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "pnpm build && run-p dev:*",
    "start:dev": "pnpm docker && pnpm db! && pnpm dev",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "pnpm db:types && pnpm prisma migrate deploy && pnpm prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:300 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "types": "pnpm db:types && tsc --noEmit -p .",
    "types:dev": "pnpm db:types && tsc --noEmit -p . --watch",
    "db:types": "pnpm exec prisma generate",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "pnpm exec prisma db seed",
    "db": "pnpm db:types && pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm exec prisma db push --force-reset",
    "db!": "pnpm db:reset && pnpm db",
    "db:sql": "dotenv -e .env.development pnpm exec prisma migrate dev --create-only",
    "db:studio": "dotenv -e .env.development pnpm exec prisma studio",
    "db:repl": "dotenv -e .env.development pnpm exec prisma-repl",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/server": "11",
    "@mantine/core": "5.10.3",
    "@mantine/dates": "5.10.3",
    "@mantine/form": "5.10.3",
    "@mantine/hooks": "5.10.3",
    "@mantine/modals": "5.10.3",
    "@mantine/notifications": "5.10.3",
    "@mantine/remix": "5.10.3",
    "@prisma/client": "4.9.0",
    "@remix-run/express": "1.12.0",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/server-runtime": "1.12.0",
    "@srtp/core": "workspace:*",
    "@srtp/fetch": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/form": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/table": "workspace:*",
    "@srtp/remix-core": "workspace:*",
    "@srtp/remix-react": "workspace:*",
    "@srtp/remix-node": "workspace:*",
    "@srtp/validator": "workspace:*",
    "@tabler/icons-react": "2.2.0",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dayjs": "1.11.7",
    "devalue": "4.2.3",
    "express": "4.18.2",
    "express-prometheus-middleware": "1.2.0",
    "isbot": "3.6.5",
    "morgan": "1.10.0",
    "prom-client": "14.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "tiny-invariant": "1.3.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.2",
    "@types/eslint": "8.21.0",
    "@types/express": "4.17.17",
