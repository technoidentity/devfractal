{
  "name": "form-examples",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf .turbo node_modules dist",
    "dev": "vite",
    "build": "pnpm types && vite build",
    "serve": "vite preview",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 --cache src tests --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "types": "tsc --noEmit -p .",
    "types:dev": "tsc --noEmit -p . --watch",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "cd": "pnpm turbo types lint test",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@mantine/core": "6.0.17",
    "@mantine/dates": "6.0.17",
    "@mantine/form": "6.0.17",
    "@mantine/hooks": "6.0.17",
    "@mantine/notifications": "6.0.17",
    "@srtp/core": "workspace:*",
    "@srtp/form": "workspace:*",
    "@srtp/global-state": "workspace:*",
    "@srtp/local-state": "workspace:*",
    "@srtp/spec": "workspace:*",
    "@srtp/validator": "workspace:*",
    "@tanstack/react-query": "4.32.0",
    "dayjs": "1.11.9",
    "immer": "10.0.2",
    "jotai": "2.2.3",
    "jotai-tanstack-query": "0.7.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-router-dom": "6.14.2",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.1",
    "type-fest": "4.1.0",
    "use-immer": "0.9.0",
    "uuid": "9.0.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/lodash": "4.14.196",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.2.0",
