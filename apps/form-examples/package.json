    "eslint-config-srtp": "workspace:*",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "48.0.1",
    "prettier": "3.0.0",
    "rimraf": "5.0.1",
    "turbo": "1.10.12",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
