{
  "name": "ui-examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "types:dev": "tsc --noEmit -p . --watch",
    "types": "tsc --noEmit -p .",
    "lint": "eslint --fix --report-unused-disable-directives --max-warnings=0 --cache src --ext .ts,.tsx",
    "lint:dev": "cross-env TIMING=1 pnpm lint --quiet --fix",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "clean": "rimraf .turbo node_modules dist",
    "deploy": "vercel deploy dist --team=turborepo --confirm",
    "db": "json-server --port 8080 --watch ./db.js"
  },
  "dependencies": {
    "@srtp/core": "workspace:*",
    "@srtp/fn": "workspace:*",
    "@srtp/ui": "workspace:*",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-router-dom": "6.15.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "tslib": "2.6.2",
    "use-immer": "0.9.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@srtp/tsconfig": "workspace:*",
    "@testing-library/react": "14.0.0",
    "@types/chance": "1.1.3",
    "@types/node": "20.5.6",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-window": "1.8.5",
    "@types/react-window-infinite-loader": "1.0.6",
    "@types/uuid": "9.0.2",
    "@vitejs/plugin-react-swc": "3.3.2",
    "cross-env": "7.0.3",
    "eslint-config-srtp": "workspace:*",
    "json-server": "0.17.3",
    "msw": "1.2.4",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "typescript": "5.2.2",
    "uuid": "9.0.0",
    "vite": "4.4.9",
    "vitest": "0.34.3"
  }
}
